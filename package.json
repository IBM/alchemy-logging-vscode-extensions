{
  "name": "alog-code-generator",
  "displayName": "Alchemy logging extension",
  "description": "Automatic log code generator for alchemy logging (alog): https://github.com/IBM/alchemy-logging",
  "version": "0.3.3",
  "publisher": "Gaurav-Kumbhat",
  "repository": {
    "type": "git",
    "url": "https://github.com/IBM/alchemy-logging-vscode-extensions.git"
  },
  "license": "SEE LICENSE IN LICENSE",
  "engines": {
    "vscode": "^1.65.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "alog",
    "alchemy logging"
  ],
  "activationEvents": [
    "onLanguage:python"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.config_log_code_prefix",
        "title": "Configure Alog Code Generator - Log Prefix"
      }
    ],
    "keybindings": [
      {
        "command": "extension.log_code",
        "key": "ctrl+shift+a",
        "mac": "cmd+shift+a",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "./node_modules/tslint/bin/tslint --project . --config ./tslint.json",
    "watch": "tsc -watch -p ./"
  },
  "dependencies": {
    "@types/node": "14.x",
    "@types/vscode": "^1.65.0",
    "ts-loader": "^9.2.8",
    "tslint": "^6.1.3",
    "typescript": "^4.6.4",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/vscode": "^1.65.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vscode/test-electron": "^2.1.3",
    "glob": "^7.2.0",
    "mocha": "^9.2.2"
  }
}
