{
  "name": "alog-code-generator",
  "displayName": "Alog-Code Generator",
  "description": "Automatic log code generator",
  "version": "0.0.1",
  "publisher": "Gaurav Kumbhat",
  "engines": {
    "vscode": "^1.65.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:python"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.log_code",
        "title": "Alog Code Generator"
      }
    ],
    "keybindings": [
      {
        "command": "extension.log_code",
        "key": "ctrl+shift+a",
        "mac": "cmd+shift+a",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "./node_modules/tslint/bin/tslint --project . --config ./tslint.json",
    "watch": "tsc -watch -p ./"
  },
  "dependencies": {
    "@types/node": "14.x",
    "@types/vscode": "^1.65.0",
    "ts-loader": "^9.2.8",
    "tslint": "^6.1.3",
    "typescript": "^4.6.4",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/vscode": "^1.65.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vscode/test-electron": "^2.1.3",
    "glob": "^7.2.0",
    "mocha": "^9.2.2"
  }
}
